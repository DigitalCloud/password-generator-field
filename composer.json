{
    "name": "digitalcloud/nova-password-generation-field",
    "description": "Nova Password Generation Field",
    "keywords": [
        "laravel",
        "nova",
        "password generation",
        "field"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Digitalcloud\\NovaPasswordGenerationField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Digitalcloud\\NovaPasswordGenerationField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
